-- Scripts:

local function COVFAW_fake_script() -- YkrastBUSTRA.LocalScript 
	local script = Instance.new('LocalScript', YkrastBUSTRA)

	-- LocalScript внутри TextButton (например, в StarterGui -> ScreenGui -> TextButton)
	
	local button = script.Parent
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local StealItemEvent = ReplicatedStorage:WaitForChild("StealItemEvent")
	local Players = game:GetService("Players")
	
	-- Функция, вызываемая при нажатии кнопки
	local function onButtonClicked()
		-- Для примера: крадём у случайного игрока (можно заменить на выбор через UI)
		local allPlayers = Players:GetPlayers()
		local validTargets = {}
		for _, p in pairs(allPlayers) do
			if p ~= Players.LocalPlayer then
				table.insert(validTargets, p)
			end
		end
	
		if #validTargets > 0 then
			local target = validTargets[math.random(1, #validTargets)]
			StealItemEvent:FireServer(target.Name)
			print("Попытка украсть предмет у " .. target.Name)
			script.Parent.Parent.FrameMoneta.Visible = true
		else
			print("Нет других игроков для кражи!")
		end
	end
	
	button.MouseButton1Click:Connect(onButtonClicked)
end
coroutine.wrap(COVFAW_fake_script)()
local function GWQYQH_fake_script() -- GaniPositionPedik.LocalScript 
	local script = Instance.new('LocalScript', GaniPositionPedik)

	local button = script.Parent
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local Players = game:GetService("Players")
	local CopyPositionEvent = ReplicatedStorage:WaitForChild("CopyPositionEvent")
	
	local function onButtonClicked()
		-- Выбираем случайного игрока (можно заменить на выбор через UI)
		local allPlayers = Players:GetPlayers()
		local validTargets = {}
		for _, p in pairs(allPlayers) do
			if p ~= Players.LocalPlayer then
				table.insert(validTargets, p)
			end
		end
	
		if #validTargets > 0 then
			local target = validTargets[math.random(1, #validTargets)]
			CopyPositionEvent:FireServer(target.Name)
			print("Копирование позиции на " .. target.Name)
		else
			print("Нет других игроков!")
		end
	end
	
	button.MouseButton1Click:Connect(onButtonClicked)
end
coroutine.wrap(GWQYQH_fake_script)()
local function WWVG_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local button = script.Parent
	local Players = game:GetService("Players")
	
	local function onButtonClicked()
		script.Parent.Parent.Parent.FrameCharacter.Visible = true
		script.Parent.Parent.Visible = false
	end
	
	button.MouseButton1Click:Connect(onButtonClicked)
end
coroutine.wrap(WWVG_fake_script)()
