coroutine.wrap(JLLG_fake_script)()
local function DPTYIXN_fake_script() -- EbatNahui.LocalScript 
	local script = Instance.new('LocalScript', EbatNahui)

	-- Местоположение: LocalScript внутри TextButton
	
	local button = script.Parent -- Получаем TextButton, в котором находится скрипт
	local Players = game:GetService("Players")
	
	-- Функция, которая срабатывает при нажатии на кнопку
	local function onButtonClicked()
		local player = Players.LocalPlayer -- Получаем локального игрока
		local character = player.Character -- Получаем персонажа игрока
		if character then
			local humanoidRootPart = character:FindFirstChild("HumanoidRootPart") -- Находим корневую часть персонажа
			local humanoid = character:FindFirstChildOfClass("Humanoid") -- Находим компонент Humanoid
			if humanoidRootPart and humanoid then
				-- Применяем импульс вперёд (в направлении, куда смотрит персонаж)
				local direction = humanoidRootPart.CFrame.LookVector -- Направление взгляда персонажа
				local impulse = direction * 150 -- Сила рывка (можно настроить)
				local bodyVelocity = Instance.new("BodyVelocity") -- Создаём временный BodyVelocity
				bodyVelocity.MaxForce = Vector3.new(math.huge, 0, math.huge) -- Ограничиваем силу по оси Y
				bodyVelocity.Velocity = impulse -- Задаём вектор скорости
				bodyVelocity.Parent = humanoidRootPart -- Привязываем к персонажу
				-- Уничтожаем BodyVelocity через короткое время, чтобы рывок был кратким
				game:GetService("Debris"):AddItem(bodyVelocity, 0.2)
				print("Рывок выполнен!") -- Выводим сообщение в консоль
			end
		end
	end
	
	-- Привязываем функцию к событию нажатия кнопки
	button.MouseButton1Click:Connect(onButtonClicked)
end
coroutine.wrap(DPTYIXN_fake_script)()
local function TFPPHF_fake_script() -- GovnoSpeed.LocalScript 
	local script = Instance.new('LocalScript', GovnoSpeed)

	-- Местоположение: LocalScript внутри TextButton
	
	local button = script.Parent -- Получаем TextButton, в котором находится скрипт
	local Players = game:GetService("Players")
	
	-- Функция, которая срабатывает при нажатии на кнопку
	local function onButtonClicked()
		local player = Players.LocalPlayer -- Получаем локального игрока
		local character = player.Character -- Получаем персонажа игрока
		if character then
			local humanoid = character:FindFirstChildOfClass("Humanoid") -- Находим компонент Humanoid
			if humanoid then
				humanoid.WalkSpeed = 150 -- Устанавливаем скорость передвижения на 150
				print("WalkSpeed установлен на 150!") -- Выводим сообщение в консоль для проверки
			end
		end
	end
	
	-- Привязываем функцию к событию нажатия кнопки
	button.MouseButton1Click:Connect(onButtonClicked)
end
coroutine.wrap(TFPPHF_fake_script)()
local function DRYAL_fake_script() -- ButtonYes.LocalScript 
	local script = Instance.new('LocalScript', ButtonYes)

	local Player = game:GetService'Players'.LocalPlayer;
	local UIS = game:GetService'UserInputService';
	
	_G.JumpHeight = 50;
	
	function Action(Object, Function) if Object ~= nil then Function(Object); end end
	
	UIS.InputBegan:connect(function(UserInput)
		if UserInput.UserInputType == Enum.UserInputType.Keyboard and UserInput.KeyCode == Enum.KeyCode.Space then
			Action(Player.Character.Humanoid, function(self)
				if self:GetState() == Enum.HumanoidStateType.Jumping or self:GetState() == Enum.HumanoidStateType.Freefall then
					Action(self.Parent.HumanoidRootPart, function(self)
						self.Velocity = Vector3.new(0, _G.JumpHeight, 0);
					end)
				end
			end)
		end
	end)
end
coroutine.wrap(DRYAL_fake_script)()
